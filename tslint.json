{
     "extends": [
          "tslint:recommended",
          "tslint-react",
          "tslint-config-prettier"
     ],
     "rules": {
          "member-access": false,
          "no-any": false,
          "object-literal-sort-keys": false,
          "ordered-imports": false,
          "no-console": [false, "log", "error"],
          "no-string-literal": false
     },
     "linterOptions": {
          "exclude": [
               "config/**/*.js",
               "node_modules/**/*.ts",
               "coverage/lcov-report/*.js"
          ]
     }
}
